      noBathroom = document.getElementById("bathroom").value;
      for (var i = 0; i < rooms.length; i++) {
        if (rooms[i].checked) {
          addRooms.push(rooms[i].value)
        }
      }
      for (var i = 0; i < noBedroom; i++) {
        img("bedroomPics");
