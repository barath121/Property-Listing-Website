<%- include('./layouts/imports') -%>
  <!-- <div class="progress">
    <div class="progress-bar progress-bar-striped  bg-danger font-weight-bold" role="progressbar" id="progress"
      style="width: 33%;" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">step 1</div>
  </div> -->

  <div class="uk-background-muted p-4">
    <div class="customLoader d-none" id="loader">
      <span uk-spinner="ratio: 4.5"></span>
    </div>
    <div class="container-fluid uk-width-1-2@m uk-background-default p-4">
      <form action="/property/createproperty" enctype="multipart/form-data" method="POST">
        <% if(property) {%>
          <input type="hidden" name="_id" value="<%= property._id %>">
          <%}%>
            <div id="1" class="">
              <h1 class="font-weight-bold">Property Details</h1>
              <br>
              <br>
              <label for="PropertyType">Property type</label>
              <br>
              <br>
              <select class="form-control w-50" name="PropertyType" id="PropertyType" required>
                <option value="">Select</option>
                <% if(property.propertyType=="Flat/Apartment" ){ %>
                  <option value="Flat/Apartment" selected> Flat/Apartment </option>
                  <%}else{%>
                    <option value="Flat/Apartment"> Flat/Apartment </option>
                    <%}%>
                      <% if(property.propertyType=="Residential House" ){ %>
                        <option value="Residential House" selected> Residential House </option>
                        <%}else{%>
                          <option value="Residential House"> Residential House </option>
                          <%}%>
                            <% if(property.propertyType=="Villa" ){ %>
                              <option value="Villa" selected> Villa </option>
                              <%}else{%>
                                <option value="Villa"> Villa </option>
                                <%}%>
                                  <% if(property.propertyType=="Builder Floor Apartment" ){ %>
                                    <option value="Builder Floor Apartment" selected>Builder Floor Apartment </option>
                                    <%}else{%>
                                      <option value="Builder Floor Apartment">Builder Floor Apartment </option>
                                      <%}%>
                                        <% if(property.propertyType=="Penthouse" ){ %>
                                          <option value="Penthouse" selected> Penthouse </option>
                                          <%}else{%>
                                            <option value="Penthouse"> Penthouse </option>
                                            <%}%>
                                              <% if(property.propertyType=="Studio Apartment" ){ %>
                                                <option value="Studio Apartment" selected> Studio Apartment </option>
                                                <%}else{%>
                                                  <option value="Studio Apartment"> Studio Apartment </option>
                                                  <%}%>
                                                    <% if(property.propertyType=="Farm House" ){ %>
                                                      <option value="Farm House" selected> Farm House </option>
                                                      <%}else{%>
                                                        <option value="Farm House"> Farm House </option>
                                                        <%}%>
              </select>

              <h4>Property For</h4>
              <div class="form-check form-check-inline">
                <% if(property.propertyFor=="Sale" ){ %>
                  <input class="form-check-input" type="radio" name="Propertyfor" onchange="saleSelected()" value="Sale"
                    id="PropertyforSale" checked required>
                  <%}else{%>
                    <input class="form-check-input" type="radio" name="Propertyfor" onchange="saleSelected()"
                      value="Sale" id="PropertyforSale" required>
                    <%}%>
                      <label class="form-check-label" for="PropertyforSale">Sale</label>
              </div>
              <div class="form-check form-check-inline">
                <% if(property.propertyFor=="Rent/Lease" ){ %>
                  <input class="form-check-input" type="radio" name="Propertyfor" onchange="RentSelected()"
                    value="Rent/Lease" id="PropertyforRent" checked>
                  <%}else{%>
                    <input class="form-check-input" type="radio" name="Propertyfor" onchange="RentSelected()"
                      value="Rent/Lease" id="PropertyforRent">
                    <%}%>
                      <label class="form-check-label" for="PropertyforRent">Rent</label>
              </div>
              <br>
              <br>
              <label for="Locality">Locality</label>
              <br>
              <br>
              <select class="form-control w-50" name="Locality" id="Locality" required>
                <% if(property.locality=="Roadpali" ){ %>
                  <option value="Roadpali" selected>Roadpali</option>
                  <%}else{%>
                    <option value="Roadpali">Roadpali</option>
                    <%}%>
                      <% if(property.locality=="Kalamboli" ){ %>
                        <option value="Kalamboli" selected>Kalamboli</option>
                        <%}else{%>
                          <option value="Kalamboli">Kalamboli</option>
                          <%}%>
                            <% if(property.locality=="Kharghar" ){ %>
                              <option value="Kharghar" selected>Kharghar</option>
                              <%}else{%>
                                <option value="Kharghar">Kharghar</option>
                                <%}%>
              </select>
              <br>
              <h3>Property Features</h3>
              <label for="name">Name</label>
              <br>
              <% if(property.name){ %>
                <input type="text" placeholder="Enter name of property" class="form-control w-50" id="name" name="name"
                  value="<%= property.name %>" required>
                <%}else{%>
                  <input type="text" placeholder="Enter name of property" class="form-control w-50" id="name"
                    name="name" required>
                  <%}%>

                    <br>
                    <br>
                    <label for="address">Address</label>
                    <br>
                    <% if(property.address){ %>
                      <input type="text" placeholder="Enter address" class="form-control w-50" id="address"
                        name="address" value="<%=property.address%>" required>
                      <%}else{%>
                        <input type="text" placeholder="Enter address" class="form-control w-50" id="address"
                          name="address" required>
                        <%}%>
                          <br>
                          <br>
                          <div>
                            <label for="Bedrooms">Bedrooms</label>
                            <br>
                            <select class="form-control w-50" onchange="dimensions()" name="Bedrooms" id="Bedrooms"
                              required>
                              <% if(property && property.propertyFeatures.bedrooms=="0" ){ %>
                                <option value="0" selected>0</option>
                                <% } else { %>
                                  <option value="0">0</option>
                                  <% } %>
                                    <% if(property && property.propertyFeatures.bedrooms=="1" ){ %>
                                      <option value="1" selected>1</option>
                                      <%}else {%>
                                        <option value="1">1</option>
                                        <%}%>
                                          <% if(property && property.propertyFeatures.bedrooms=="2" ){ %>
                                            <option value="2" selected>2</option>
                                            <%}else {%>
                                              <option value="2">2</option>
                                              <%}%>
                                                <% if(property && property.propertyFeatures.bedrooms=="3" ){ %>
                                                  <option value="3" selected>3</option>
                                                  <%}else {%>
                                                    <option value="3">3</option>
                                                    <%}%>
                                                      <% if(property && property.propertyFeatures.bedrooms=="4" ){ %>
                                                        <option value="4" selected>4</option>
                                                        <%}else {%>
                                                          <option value="4">4</option>
                                                          <%}%>
                                                            <% if(property &&
                                                              property.propertyFeatures.bedrooms=="more than 4" ){ %>
                                                              <option value="more than 4" selected>more than 4</option>
                                                              <%}else {%>
                                                                <option value="more than 4">more than 4</option>
                                                                <%}%>
                            </select>
                            <div id="tableDivBed" class="d-none">
                              <table class="table w-50">
                                <thead>
                                  <th scope="col">#</th>
                                  <th scope="col">Length</th>
                                  <th scope="col">Breadth</th>
                                  <th scope="col">Area</th>
                                </thead>
                                <tbody id="bedDimensions">

                                </tbody>
                              </table>
                            </div>
                            <br>
                            <label for="Balconies">Balconies</label>
                            <br>
                            <select class="form-control w-50" onchange="balconies()" name="Balconies" id="Balconies"
                              required>
                              <% if(property && property.propertyFeatures.balconies=="0" ){ %>
                                <option value="0" selected>0</option>
                                <%}else {%>
                                  <option value="0">0</option>
                                  <%}%>
                                    <% if(property && property.propertyFeatures.balconies=="1" ){ %>
                                      <option value="1" selected>1</option>
                                      <%}else {%>
                                        <option value="1">1</option>
                                        <%}%>
                                          <% if(property && property.propertyFeatures.balconies=="2" ){ %>
                                            <option value="2" selected>2</option>
                                            <%}else {%>
                                              <option value="2">2</option>
                                              <%}%>
                                                <% if(property && property.propertyFeatures.balconies=="3" ){ %>
                                                  <option value="3" selected>3</option>
                                                  <%}else {%>
                                                    <option value="3">3</option>
                                                    <%}%>
                                                      <% if(property && property.propertyFeatures.balconies=="4" ){ %>
                                                        <option value="4" selected>4</option>
                                                        <%}else {%>
                                                          <option value="4">4</option>
                                                          <%}%>
                                                            <% if(property &&
                                                              property.propertyFeatures.balconies=="more than 4" ){ %>
                                                              <option value="more than 4" selected>more than 4</option>
                                                              <%}else {%>
                                                                <option value="more than 4">more than 4</option>
                                                                <%}%>
                            </select>
                            <br>
                            <label for="bathroom">Bathroom</label>
                            <br>
                            <select class="form-control w-50" onchange="bathrooms()" name="bathroom" id="bathroom"
                              required>
                              <% if(property && property.propertyFeatures.bathroom=="0" ){ %>
                                <option value="0" selected>0</option>
                                <%}else {%>
                                  <option value="0">0</option>
                                  <%}%>
                                    <% if(property && property.propertyFeatures.bathroom=="1" ){ %>
                                      <option value="1" selected>1</option>
                                      <%}else {%>
                                        <option value="1">1</option>
                                        <%}%>
                                          <% if(property && property.propertyFeatures.bathroom=="2" ){ %>
                                            <option value="2" selected>2</option>
                                            <%}else {%>
                                              <option value="2">2</option>
                                              <%}%>
                                                <% if(property && property.propertyFeatures.bathroom=="3" ){ %>
                                                  <option value="3" selected>3</option>
                                                  <%}else {%>
                                                    <option value="3">3</option>
                                                    <%}%>
                                                      <% if(property && property.propertyFeatures.bathroom=="4" ){ %>
                                                        <option value="4" selected>4</option>
                                                        <%}else {%>
                                                          <option value="4">4</option>
                                                          <%}%>
                                                            <% if(property &&
                                                              property.propertyFeatures.bathroom=="more than 4" ){ %>
                                                              <option value="more than 4" selected>more than 4</option>
                                                              <%}else {%>
                                                                <option value="more than 4">more than 4</option>
                                                                <%}%>
                            </select>
                            <br>
                          </div>
                          <br>
                          <label for="floorNo">Floor No.</label>
                          <br>
                          <% if(property && property.propertyFeatures.floorNo){ %>
                            <input type="text" class="form-control w-50" onchange="FloorNo()" pattern="^[0-9]*$"
                              title="Please enter a valid number" placeholder="Enter Floor number" id="floorNo"
                              name="floorNo" value="<%= property.propertyFeatures.floorNo %>" required>
                            <%}else{%>
                              <input type="text" class="form-control w-50" onchange="FloorNo()" pattern="^[0-9]*$"
                                title="Please enter a valid number" placeholder="Enter Floor number" id="floorNo"
                                name="floorNo" required>
                              <%}%>
                                <br>
                                <br>
                                <label for="totalFloors">Total Floors</label>
                                <br>
                                <% if( property && property.propertyFeatures.totalFloors){ %>
                                  <input type="text" onchange="totalFloor()" class="form-control w-50"
                                    pattern="^[0-9]*$" title="Please enter a valid number"
                                    placeholder="Enter Total Floors" id="totalFloors" name="totalFloors"
                                    value="<%= property.propertyFeatures.totalFloors %>" required>
                                  <%}else{%>
                                    <input type="text" onchange="totalFloor()" class="form-control w-50"
                                      pattern="^[0-9]*$" title="Please enter a valid number"
                                      placeholder="Enter Total Floors" id="totalFloors" name="totalFloors" required>
                                    <%}%>
                                      <br>
                                      <br>
                                      <label for="flatsOnFloor">Flats On Floor</label>
                                      <br>
                                      <% if( property && property.propertyFeatures.totalFloors){ %>
                                        <input type="text" class="form-control w-50"
                                          placeholder="Enter The Number of Flats on the floor" pattern="^[0-9]*$"
                                          title="Please enter a valid number" id="flatsOnFloor" name="flatsOnFloor"
                                          value="<%= property.propertyFeatures.flatsOnFloor %>" required>
                                        <%}else{%>
                                          <input type="text" class="form-control w-50"
                                            placeholder="Enter The Number of Flats on the floor" pattern="^[0-9]*$"
                                            title="Please enter a valid number" id="flatsOnFloor" name="flatsOnFloor"
                                            required>
                                          <%}%>
                                            <br>
                                            <br>
                                            <label for="furnishingStatus">Furnishing Status</label>
                                            <br>
                                            <select class="form-control w-50" name="furnishingStatus"
                                              id="furnishingStatus" required>
                                              <% if(property && property.propertyFeatures.furnishingStatus=="" ){ %>
                                                <option value="" selected>Select</option>
                                                <%}else {%>
                                                  <option value="">Select</option>
                                                  <%}%>
                                                    <% if(property &&
                                                      property.propertyFeatures.furnishingStatus=="Furnished" ){ %>
                                                      <option value="Furnished" selected>Furnished</option>
                                                      <%}else {%>
                                                        <option value="Furnished">Furnished</option>
                                                        <%}%>
                                                          <% if(property &&
                                                            property.propertyFeatures.furnishingStatus=="Unfurnished" ){
                                                            %>
                                                            <option value="Unfurnished" selected>Unfurnished</option>
                                                            <%}else {%>
                                                              <option value="Unfurnished">Unfurnished</option>
                                                              <%}%>
                                                                <% if(property &&
                                                                  property.propertyFeatures.furnishingStatus=="Semi-Furnished"
                                                                  ){ %>
                                                                  <option value="Semi-Furnished" selected>Semi-Furnished</option>
                                                                  <%}else {%>
                                                                    <option value="Semi-Furnished">Semi-Furnished
                                                                    </option>
                                                                    <%}%>
                                            </select>
                                            <br>
                                            <h3>Furniture</h3>
                                            <div class="row">
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="TV" id="TV">
                                                <label class="form-check-label" for="TV">
                                                  TV
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="TV unit" id="TV unit">
                                                <label class="form-check-label" for="TV unit">
                                                  TV unit
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="Sofa" id="Sofa">
                                                <label class="form-check-label" for="Sofa">
                                                  Sofa
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="Dining Table" id="Dining Table">
                                                <label class="form-check-label" for="Dining Table">
                                                  Dining Table
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" onchange="fan()" name="furniture"
                                                  type="checkbox" value="Fan" id="Fan.">
                                                <label class="form-check-label" for="Fan.">
                                                  Fan
                                                </label>
                                                <input type="text" placeholder="enter quantity" pattern="^[0-9]*$"
                                                  title="Please enter a valid number" class="form-control w-50 d-none"
                                                  id="fanAmount" name="Quantity">
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" onchange="light()" name="furniture"
                                                  type="checkbox" value="Light" id="Light">
                                                <label class="form-check-label" for="Light">
                                                  Light
                                                </label>
                                                <input type="text" placeholder="enter quantity" pattern="^[0-9]*$"
                                                  title="Please enter a valid number" class="form-control w-50 d-none"
                                                  id="lightAmount" name="Quantity">
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="Modular Kitchen" id="Modular Kitchen">
                                                <label class="form-check-label" for="Modular Kitchen">
                                                  Modular Kitchen
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="Chimney" id="Chimney">
                                                <label class="form-check-label" for="Chimney">
                                                  Chimney
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="Microwave" id="Microwave">
                                                <label class="form-check-label" for="Microwave">
                                                  Microwave
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="Fridge" id="Fridge">
                                                <label class="form-check-label" for="Fridge">
                                                  Fridge
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" name="furniture" type="checkbox"
                                                  value="Washing machine" id="Washingmachine">
                                                <label class="form-check-label" for="Washingmachine">
                                                  Washing machine
                                                </label>
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" onchange="bed()" name="furniture"
                                                  type="checkbox" value="Bed" id="Bed">
                                                <label class="form-check-label" for="Bed">
                                                  Bed
                                                </label>
                                                <input type="text" placeholder="enter quantity" pattern="^[0-9]*$"
                                                  title="Please enter a valid number" class="form-control w-50 d-none"
                                                  id="bedAmount" name="Quantity">
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" onchange="wadrobe()" name="furniture"
                                                  type="checkbox" value="Wadrobe" id="Wadrobe">
                                                <label class="form-check-label" for="Wadrobe">
                                                  Wadrobe
                                                </label>
                                                <input type="text" placeholder="enter quantity" pattern="^[0-9]*$"
                                                  title="Please enter a valid number" class="form-control w-50 d-none"
                                                  id="wadrobeAmount" name="Quantity">
                                              </div>
                                              <div class="form-check col-6">
                                                <input class="form-check-input" onchange="curtains()" name="furniture"
                                                  type="checkbox" value="Curtains" id="Curtains">
                                                <label class="form-check-label" for="Curtains">
                                                  Curtains
                                                </label>
                                                <input type="text" placeholder="enter quantity" pattern="^[0-9]*$"
                                                  title="Please enter a valid number" class="form-control w-50 d-none"
                                                  id="curtainsAmount" name="Quantity">
                                              </div>
                                            </div>
                                            <br>
                                            <h3>Area</h3>

                                            <label for="carpetArea">Carpet Area</label>
                                            <br>
                                            <div class="input-group w-50">
                                              <% if(property && property.propertyFeatures.carpetArea){ %>
                                              <input type="text" id="carpetArea" pattern="^[0-9]*$"
                                                title="Please enter a valid number"
                                                class="form-control border-top-0 border-left-0 border-right-0"
                                                name="carpetArea" value="<%= property.propertyFeatures.carpetArea %>" required>
                                                  <%} else {%>
                                                    <input type="text" id="carpetArea" pattern="^[0-9]*$"
                                                title="Please enter a valid number"
                                                class="form-control border-top-0 border-left-0 border-right-0"
                                                name="carpetArea" required>
                                                    <%}%>
                                                    <span>
                                                <select name="carpetArea"
                                                  class="form-control border-top-0 border-left-0 border-right-0"
                                                  id="furnishingStatus">
                                                  <option value="Sqft">Sqft</option>
                                                  <option value="Acre">Acre</option>
                                                  <option value="Sq-M">Sq-M</option>
                                                </select>
                                              </span>
                                            </div>
                                            <br>
                                            <br>
                                            <label for="superBuiltUpArea">Super BuiltUp Area</label>
                                            <br>
                                            <div class="input-group w-50">
                                              <% if(property && property.propertyFeatures.superBuiltUpArea){ %>
                                              <input type="text" id="superBuiltUpArea" pattern="^[0-9]*$"
                                                title="Please enter a valid number"
                                                class="form-control border-top-0 border-left-0 border-right-0"
                                                name="superBuiltUpArea" value="<%= property.propertyFeatures.superBuiltUpArea %>">
                                                <%} else {%>
                                                  <input type="text" id="superBuiltUpArea" pattern="^[0-9]*$"
                                                title="Please enter a valid number"
                                                class="form-control border-top-0 border-left-0 border-right-0"
                                                name="superBuiltUpArea">
                                                  <%}%>
                                                <span>
                                                <select name="superBuiltUpArea"
                                                  class="form-control border-top-0 border-left-0 border-right-0"
                                                  id="superBuiltUpArea">
                                                  <option value="Sqft">Sqft</option>
                                                  <option value="Acre">Acre</option>
                                                  <option value="Sq-M">Sq-M</option>
                                                </select>
                                              </span>
                                            </div>

                                            <br>
                                            <br>
                                            <label for="builtUpArea">BuiltUp Area</label>
                                            <br>
                                            <div class="input-group w-50">
                                              <% if(property && property.propertyFeatures.builtUpArea){ %>
                                              <input type="text" id="builtUpArea" pattern="^[0-9]*$"
                                                title="Please enter a valid number"
                                                class="form-control border-top-0 border-left-0 border-right-0"
                                                name="builtUpArea" value="<%= property.propertyFeatures.builtUpArea %>">
                                                <%} else {%>
                                                  <input type="text" id="builtUpArea" pattern="^[0-9]*$"
                                                title="Please enter a valid number"
                                                class="form-control border-top-0 border-left-0 border-right-0"
                                                name="builtUpArea">
                                                  <%}%>
                                                  <span>
                                                <select name="builtUpArea"
                                                  class="form-control border-top-0 border-left-0 border-right-0 "
                                                  class="form-control" id="builtUpArea">
                                                  <option value="Sqft">Sqft</option>
                                                  <option value="Acre">Acre</option>
                                                  <option value="Sq-M">Sq-M</option>
                                                </select>
                                              </span>

                                            </div>
                                            <br>
                                            <br>


                                            <h3>Price Details</h3>
                                            <% if(property && property.propertyFor=="Sale" ){ %>
                                            <div id="sale">
                                              <label for="expectedPrice">Expected Price</label>
                                              <br>
                                              <input type="text" id="expectedPrice" pattern="^[0-9]*$"
                                                title="Please enter a valid number"
                                                class="form-control border-top-0 border-left-0 border-right-0 w-50"
                                                name="expectedPrice" value="<%= property.priceDetails.expectedPrice %>" placeholder="Enter total price"><span>
                                                <br>
                                                <br>
                                                <label for="bookingAmount">Booking Amount</label>
                                                <br>
                                                <input type="text" id="bookingAmount" pattern="^[0-9]*$"
                                                  title="Please enter a valid number"
                                                  class="form-control border-top-0 border-left-0 border-right-0 w-50"
                                                  name="bookingAmount" value="<%= property.priceDetails.bookingAmount %> placeholder="Enter total price"><span>
                                                  <br>
                                                  <br>
                                                  <label>Transaction Type</label>
                                                  <br>
                                                  <div class="form-check form-check-inline">
                                                    <% if(property.priceDetails.transactionType=="New Property" ){ %>
                                                    <input class="form-check-input" type="radio" name="transactionType"
                                                      value="New Property" id="transactionTypenew" checked>
                                                      <%} else {%>
                                                        <input class="form-check-input" type="radio" name="transactionType"
                                                      value="New Property" id="transactionTypenew">
                                                        <%}%>
                                                    <label class="form-check-label" for="transactionTypenew">New
                                                      Property</label>
                                                  </div>
                                                  <div class="form-check form-check-inline">
