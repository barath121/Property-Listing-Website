                <%}else{%>
                  <input type="text" placeholder="Enter name of property" class="form-control w-50 " id="name"
                    name="name" required>
                  <%}%>

                    <br>
                    <br>
                    <label for="address">Address</label>
                    <br>
                    <% if(property.address){ %>
                     <textarea type="text" class="form-control w-50 " name="address" id="address" cols="30" rows="3" required><%=property.address%></textarea>
                     <%}else{%>
                      <textarea type="text" class="form-control w-50 " name="address" id="address" cols="30" rows="3"><%=property.address%></textarea>
