    function dimensions() {
      document.getElementById("bedDimensions").innerHTML = null;
      document.getElementById("tableDivBed").classList.remove("d-none");
      var tables = document.getElementById("Bedrooms").value;
      if (tables == "more than 4") {
        tables = 4;
      }
      noBedroom = tables;
      // console.log(location.href)
      if (document.getElementById("bedroomPics")) {
        document.getElementById("bedroomPics").innerHTML = null;
        for (var i = 0; i < noBedroom; i++) {
          img("bedroomPics", i);
        }
      }

      for (var i = 0; i < tables; i++) {
        var lengthVal;
        var breadthVal;
        var tr = document.createElement("tr");
        var td1 = document.createElement("td");
        var td2 = document.createElement("td");
        var td3 = document.createElement("td");
        var td4 = document.createElement("td")
        var length = document.createElement("input");
        var breadth = document.createElement("input");
        var equals = document.createElement("span");

        length.name = "lenght" + i;
        breadth.name = "breadth" + i;

        length.value = 0;
        breadth.value = 0;
        length.setAttribute('id', "Bedlength " + i);
        breadth.setAttribute('id', "Bedbreadth " + i);
        equals.setAttribute('id', "Bedequals " + i);

        length.onchange = () => {
          for (j = 0; j < tables; j++) {
            document.getElementById('Bedequals ' + j).innerHTML = (document.getElementById('Bedlength ' + j).value * document.getElementById('Bedbreadth ' + j).value) + " Sq.Ft"
          }
        }

        breadth.onchange = () => {
          for (j = 0; j < tables; j++) {
            document.getElementById('Bedequals ' + j).innerHTML = (document.getElementById('Bedlength ' + j).value * document.getElementById('Bedbreadth ' + j).value) + " Sq.Ft"
          }
        }

        length.className = "form-control"
        breadth.className = "form-control"

        td4.innerHTML = i + 1;

        td1.appendChild(length)
        td2.appendChild(breadth)
        td3.appendChild(equals)
        tr.appendChild(td4)
        tr.appendChild(td1)
        tr.appendChild(td2)
        tr.appendChild(td3)
        document.getElementById("bedDimensions").appendChild(tr)
      }

    }

