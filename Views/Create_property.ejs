        img("additionalRooms", ++indexID, room.value);
      }
      if (!room.checked) {
        document.getElementById("additionalRooms").childNodes.forEach(ele => {
          if (ele.innerText == room.value) {
            // console.log(ele.)
            document.getElementById("additionalRooms").removeChild(ele)
            document.getElementById(room.value.split(" ").join("")).innerHTML = null;
          }
        })
      }
      }
    }

