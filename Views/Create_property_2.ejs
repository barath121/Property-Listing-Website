<%- include('./layouts/imports') -%>
    <div class="uk-background-muted p-4">
        <div class="container-fluid uk-width-1-2@m uk-background-default p-4">
            <form action="/property/createproperty" enctype="multipart/form-data" method="POST">
                <h1 class="font-weight-bold">Additional Details</h1>
                <label>Additonal Rooms</label>
                <br>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" name="additonalRooms" value="Puja Room"
                        id="additonalRoomsPuja">
                    <label class="form-check-label" for="additonalRoomsPuja">Puja Room</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" name="additonalRooms" value="Study Room"
                        id="additonalRoomsStudy">
                    <label class="form-check-label" for="additonalRoomsStudy">Study Room</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" id="additonalRoomsStore" name="additonalRooms"
                        value="Store Room">
                    <label class="form-check-label" for="additonalRoomsStore">Store Room</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" id="additonalRoomsServant" name="additonalRooms"
                        value="Servant Room">
                    <label class="form-check-label" for="additonalRoomsServant">Servant Room</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" id="additonalRoomsNone" name="additonalRooms"
                        value="None of these">
                    <label class="form-check-label" for="additonalRoomsNone">None of these</label>

                    <br>

                </div>
                <br>
                <br>
                <label for="facing">Facing</label>
                <br>
                <select class="form-control w-50" name="facing" id="facing">
                    <option value="North">North</option>
                    <option value="East">East</option>
                    <option value="West">West</option>
                    <option value="South">South</option>
                    <option value="North-West">North-West</option>
                    <option value="North-East">North-East</option>
                    <option value="South-West">South-West</option>
                    <option value="South-East">South-East</option>
                </select>
                <br>
                <br>
                <label for="overlooking">Overlooking</label>
                <br>
                <select class="form-control w-50" name="overlooking" id="overlooking">
                    <option value="Garden/Park">Garden/Park</option>
                    <option value="Pool">Pool</option>
                    <option value="Main Road">Main Road</option>
                    <option value="Not Availiable">Not Availiable</option>
                </select>
                <br>
                <br>
                <label for="carParking">Car Parking</label>
                <br>
                <select class="form-control w-50" name="carParking" id="carParking">
                    <option value="Covered">Covered</option>
                    <option value="Open">Open</option>
                    <option value="None">None</option>
                    <option value="Not Availiable">Not Availiable</option>
                </select>
                <br>
                <br>
                <label for="liftsInTheTower">Lifts In The Tower</label>
                <br>
                <select class="form-control w-50" name="liftsInTheTower" id="liftsInTheTower">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="4+">4+</option>
                </select>
                <br>
                <br>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" name="multipleUnitsAvaliable" value="true"
                        id="multipleUnitsAvaliable">
                    <label class="form-check-label" for="multipleUnitsAvaliable">Multiple Units Avaliable</label>
                </div>
                <br>
                <br>
                <h3>Status Of Water and Electric</h3>
                <br>

                <label for="avaliablityOfWater">Avaliablity Of Water</label>
                <br>
                <select class="form-control w-50" name="avaliablityOfWater" id="avaliablityOfWater">
                    <option value="24 Hours Avaliable">24 Hours Avaliable</option>
                    <option value="12 Hours Avaliable">12 Hours Avaliable</option>
                    <option value="6 Hours Avaliable">6 Hours Avaliable</option>
                    <option value="2 Hours Avaliable">2 Hours Avaliable</option>
                    <option value="1 Hours Avaliabl">1 Hours Avaliabl</option>
                </select>
                <br>
                <br>

                <label for="avaliablityOfElectricity">Avaliablity Of Electricity</label>
                <br>
                <select class="form-control w-50" name="avaliablityOfElectricity" id="avaliablityOfElectricity">
                    <option value="24 Hours Avaliable">24 Hours Avaliable</option>
                    <option value="12 Hours Avaliable">12 Hours Avaliable</option>
                    <option value="6 Hours Avaliable">6 Hours Avaliable</option>
                    <option value="2 Hours Avaliable">2 Hours Avaliable</option>
                    <option value="1 Hours Avaliabl">1 Hours Avaliabl</option>
                </select>
                <br>
                <br>
                <h3>Others</h3>
                <label for="flooring">Flooring</label>
                <br>
                <select class="form-control w-50" name="flooring" id="flooring">
                    <option value="Ceramic Tiles">Ceramic Tiles</option>
                    <option value="Granite">Granite</option>
                    <option value="Marble">Marble</option>
                    <option value="Marbonite">Marbonite</option>
                    <option value="Mosaic">Mosaic</option>
                    <option value="Normal Tiles/Kotha Stone">Normal Tiles/Kotha Stone</option>
                    <option value="Vitrified">Vitrified</option>
                    <option value="Wooden">Wooden</option>
                </select>
                <br>
                <h3>Amenities</h3>
                <div class="row">
                    <div class="form-check  col-6">
                        <input class="form-check-input" type="checkbox" name="amenities" value="Air Conditioned"
                            id="AirConditioned">
                        <label class="form-check-label" for="AirConditioned">
                            Air Conditioned
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" type="checkbox" name="amenities" value="Banquet Hall"
                            id="BanquetHall">
                        <label class="form-check-label" for="BanquetHall">
                            Banquet Hall
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Bar/Lounge"
                            id="Bar/Lounge">
                        <label class="form-check-label" for="Bar/Lounge">
                            Bar/Lounge
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Cafeteria/Food Court"
                            id="Cafeteria/FoodCourt">
                        <label class="form-check-label" for="Cafeteria/FoodCourt">
                            Cafeteria/Food Court
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Club House"
                            id="ClubHouse">
                        <label class="form-check-label" for="ClubHouse">
                            Club House
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Conference Room"
                            id="ConferenceRoom">
                        <label class="form-check-label" for="ConferenceRoom">
                            Conference Room
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="DTH Television Facility"
                            id="DTH Television Facility">
                        <label class="form-check-label" for="DTH Television Facility">
                            DTH Television Facility
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Gymnasium"
                            id="Gymnasium">
                        <label class="form-check-label" for="Gymnasium">
                            Gymnasium
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Intercom Facility"
                            id="Intercom Facility">
                        <label class="form-check-label" for="Intercom Facility">
                            Intercom Facility
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox"
                            value="Internet/Wi-Fi Connectivity" id="Internet/Wi-Fi Connectivity">
                        <label class="form-check-label" for="Internet/Wi-Fi Connectivity">
                            Internet/Wi-Fi Connectivity
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox"
                            value="Jogging and Strolling Track" id="Jogging and Strolling Track">
                        <label class="form-check-label" for="Jogging and Strolling Track">
                            Jogging and Strolling Track
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Laundry Service"
                            id="Laundry Service">
                        <label class="form-check-label" for="Laundry Service">
                            Laundry Service
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Lift" id="Lift">
                        <label class="form-check-label" for="Lift">
                            Lift
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Maintenance Staff"
                            id="Maintenance Staff">
                        <label class="form-check-label" for="Maintenance Staff">
                            Maintenance Staff
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Outdoor Tennis Courts"
                            id="Outdoor Tennis Courts">
                        <label class="form-check-label" for="Outdoor Tennis Courts">
                            Outdoor Tennis Courts
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Park" id="Park">
                        <label class="form-check-label" for="Park">
                            Park
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Piped Gas"
                            id="Piped Gas">
                        <label class="form-check-label" for="Piped Gas">
                            Piped Gas
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Power Back Up"
                            id="Power Back Up">
                        <label class="form-check-label" for="Power Back Up">
                            Power Back Up
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Private Terrace/Garden"
                            id="Private Terrace/Garden">
                        <label class="form-check-label" for="Private Terrace/Garden">
                            Private Terrace/Garden
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="RO Water System"
                            id="RO Water System">
                        <label class="form-check-label" for="RO Water System">
                            RO Water System
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Rain Water Harvesting"
                            id="Rain Water Harvesting">
                        <label class="form-check-label" for="Rain Water Harvesting">
                            Rain Water Harvesting
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Reserved Parking"
                            id="Reserved Parking">
                        <label class="form-check-label" for="Reserved Parking">
                            Reserved Parking
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Security" id="Security">
                        <label class="form-check-label" for="Security">
                            Security
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Service/Goods Lift"
                            id="Service/Goods Lift">
                        <label class="form-check-label" for="Service/Goods Lift">
                            Service/Goods Lift
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Swimming Pool"
                            id="Swimming Pool">
                        <label class="form-check-label" for="Swimming Pool">
                            Swimming Pool
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Vaastu Compliant"
                            id="Vaastu Compliant">
                        <label class="form-check-label" for="Vaastu Compliant">
                            Vaastu Compliant
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Visitor Parking"
                            id="Visitor Parking">
                        <label class="form-check-label" for="Visitor Parking">
                            Visitor Parking
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Waste Disposal"
                            id="Waste Disposal">
                        <label class="form-check-label" for="Waste Disposal">
                            Waste Disposal
                        </label>
                    </div>
                    <div class="form-check  col-6">
                        <input class="form-check-input" name="amenities" type="checkbox" value="Water Storage"
                            id="Water Storage">
                        <label class="form-check-label" for="Water Storage">
                            Water Storage
                        </label>
                    </div>
                </div>
                <br>
                <br>
                <label for="description">Description</label>
                <br>
                <textarea class="form-control" name="description" id="description" cols="30" rows="5"></textarea>
                <br>
                <br>
                <label for="landmarks">Landmarks</label>
                <br>
                <input type="text" id="landmarks" class="form-control border-top-0 border-left-0 border-right-0 w-50"
                    name="landmarks" placeholder="Enter landmark"><span>
                    <br>
                    <br>
                    <div class="d-flex w-100 justify-content-center">
                        <button class="btn btn-outline-danger" type="submit">Submit</button>
                    </div>

            </form>
        </div>
    </div>
    <%- include('./layouts/Footer') -%>