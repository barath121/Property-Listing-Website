    function meetingRooms() {
      if(document.getElementById("meetingRoomsDiv")){
        document.getElementById("meetingRoomsDiv").innerHTML = null;
        for (var i = 1; i <= document.getElementById("noOfMeetingRooms").value; i++) {
        img("meetingRoomsDiv", i, "Meeting Room");
      }
      }
    }

    function conferenceRooms(check) {
      if(document.getElementById("CommercialOfficeSpacefeatures")){
        if (check.checked) {
        img("CommercialOfficeSpacefeatures", 0, "Conference Room");
      }
      if (!check.checked) {
        document.getElementById("CommercialOfficeSpacefeatures").childNodes.forEach(ele => {
          if (ele.innerText == "Conference Room") {
            // console.log(ele.)
            document.getElementById("CommercialOfficeSpacefeatures").removeChild(ele)
            document.getElementById("ConferenceRoom").innerHTML = null;
          }
        })
      }
      }
    }

    function receptionAreas(check) {
      if(document.getElementById("CommercialOfficeSpacefeatures")){
        if (check.checked) {
        img("CommercialOfficeSpacefeatures", 1, "Reception Area");
      }
      if (!check.checked) {
        document.getElementById("CommercialOfficeSpacefeatures").childNodes.forEach(ele => {
          if (ele.innerText == "Reception Area") {
            // console.log(ele.)
            document.getElementById("CommercialOfficeSpacefeatures").removeChild(ele)
            document.getElementById("ReceptionArea").innerHTML = null;
          }
        })
      }
      }
    }
    function totalFloor() {
      var floorNo = document.getElementById("yourFloor").value;
      if (+floorNo > +document.getElementById("totalFloors").value) {
        document.getElementById("totalFloors").value = ""
        document.getElementById("totalFloors").setAttribute("placeholder", "should be more than floor number");
      }

    }
    function FloorNo() {
      var floorNo = document.getElementById("yourFloor").value;
      if (document.getElementById("totalFloors").value != "") {
        if (+floorNo > +document.getElementById("totalFloors").value) {
          document.getElementById("totalFloors").value = ""
          document.getElementById("totalFloors").setAttribute("placeholder", "should be more than floor number");
        }
      }
    }
