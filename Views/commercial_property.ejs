    function img(parentDivId, callVal, rooms) {
      var parentDiv = document.getElementById(parentDivId),
        parent = document.createElement("div"),
        inpDiv = document.createElement("div"),
        inp = document.createElement("input"),
        label = document.createElement("label"),
        h5 = document.createElement("h5"),
        previewDiv = document.createElement("div"),
        childPreviewDiv = document.createElement("div"),
        previewButton = document.createElement("button"),
        previewImage = document.createElement("img");

      parent.className = "mb-2";
      inpDiv.className = "custom-file";
      inp.className = "custom-file-input";
      inp.type = "file"
      inp.accept = ".jpeg,.png,.jpg"
      inp.name = parentDivId;
      label.for = "inputGroupFile02"
      label.innerHTML = "Choose file"
      label.className = "custom-file-label"
      previewDiv.className = "d-flex w-100 justify-content-end mt-2"
      previewButton.className = "bg-transparent border-0"
      previewButton.type = "button"
      previewButton.setAttribute("uk-toggle", "target: #modal-close-outside")
      previewImage.className = "p-2"
      previewImage.style.width = "100px"
      previewImage.setAttribute("id", parentDivId + callVal + "Preview")

      previewButton.onclick = () => {
        Preview(parentDivId + callVal + "Preview")
      }

      inp.onchange = () => {
        loadFile(event, parentDivId + callVal + "Preview")
      }

      inpDiv.appendChild(inp);
      inpDiv.appendChild(label);
      previewDiv.appendChild(childPreviewDiv);
      childPreviewDiv.appendChild(previewButton);
      previewButton.appendChild(previewImage);
      parent.appendChild(inpDiv);
      parent.appendChild(previewDiv)
      parentDiv.appendChild(parent)

      if (parentDivId == "CommercialOfficeSpacefeatures") {
        var roomId = rooms.split(" ").join("");
        h5.innerHTML = rooms;
        inp.name = rooms;
        inpDiv.appendChild(inp);
        inpDiv.appendChild(label);
        previewDiv.appendChild(childPreviewDiv);
        childPreviewDiv.appendChild(previewButton);
        previewButton.appendChild(previewImage);
        parent.setAttribute("id", roomId);
        parent.appendChild(inpDiv);
        parent.appendChild(previewDiv);
        parentDiv.appendChild(h5);
        parentDiv.appendChild(parent);

      }
    }

