                <label for="passengerLifts">Passenger Lifts</label>
                <br>
                <input type="text" placeholder="Enter number of Passenger Lifts" class="form-control w-50"
                  id="passengerLifts" name="passengerLifts">
                <br>
                <label for="serviceLifts">Service Lifts</label>
                <br>
                <input type="text" placeholder="Enter number of Service Lifts" class="form-control w-50" id="serviceLifts"
                  name="serviceLifts">
              </div>
              <%}%>
          </div>
               <%} else if(property.propertyType == "Commercial Shop") {%>
