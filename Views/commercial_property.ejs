<!DOCTYPE html>
<html lang="en">

<head>
  <title>Santosh Property - Commercial Property</title>
  <link href="https://santoshproperty.in/commercialproperty" rel="canonical" />
<%- include('./layouts/imports') -%>
  <div class="uk-background-muted p-4">
    <div class="customLoader d-none" id="loader">
      <span uk-spinner="ratio: 4.5"></span>
    </div>
  <div class="uk-background-muted p-4">
    <div class="container-fluid uk-width-1-2@m uk-background-default p-4">
      <form action="/property/commercialproperty" enctype="multipart/form-data" id="propertyform" method="POST">
        <% if(property) {%>
          <input type="hidden" name="_id" value="<%= property._id %>">
        <%}%>
        <div id="errormsg" class="d-none uk-alert-danger uk-width-auto" uk-alert>
          <p class="text-center">Please Check All Inputs</p>
        </div>
        <div id="1" class="">
          <h1 class="font-weight-bold">Property Details</h1>
          <br>
          <br>
          <label for="PropertyType">Property type</label>
          <br>
          <br>
          <select class="form-control w-50" onchange="select()" name="PropertyType" id="PropertyType" required>
            <option value="">Select</option>
            <% if(property && property.propertyType == "Commercial Shop" ){ %>
            <option value="Commercial Shop" selected> Commercial Shop </option>
            <%} else {%>
              <option value="Commercial Shop"> Commercial Shop </option>
            <%}%>
            <% if(property && property.propertyType == "Commercial Office Space" ){ %>
            <option value="Commercial Office Space" selected> Commercial Office Space </option>
            <%} else {%>
              <option value="Commercial Office Space"> Commercial Office Space </option>
              <%}%>
            <% if(property && property.propertyType == "Commercial Showroom" ){ %>
            <option value="Commercial Showroom" selected> Commercial Showroom </option>
            <%} else {%>
              <option value="Commercial Showroom"> Commercial Showroom </option>
              <%}%>
              <% if(property && property.propertyType == "Warehouse/Godows" ){ %>
            <option value="Warehouse/Godows" selected>Warehouse/Godows </option>
            <%} else {%>
              <option value="Warehouse/Godows">Warehouse/Godows </option>
              <%}%>
              <% if(property && property.propertyType == "Industrial Shed" ){ %>
            <option value="Industrial Shed" selected> Industrial Shed </option>
            <%} else {%>
              <option value="Industrial Shed"> Industrial Shed </option>
              <%}%>
          </select>

          <h4>Property For</h4>
          <div class="form-check form-check-inline">
            <% if(property && property.propertyFor=="Sale" ){ %>
              <input class="form-check-input" type="radio" name="Propertyfor" onchange="saleSelected()" value="Sale"
                id="PropertyforSale" checked required>
              <%}else{%>
                <input class="form-check-input" type="radio" name="Propertyfor" onchange="saleSelected()"
                  value="Sale" id="PropertyforSale" required>
                <%}%>
                  <label class="form-check-label" for="PropertyforSale">Sale</label>
          </div>
          <div class="form-check form-check-inline">
            <% if( property && property.propertyFor=="Rent/Lease" ){ %>
              <input class="form-check-input" type="radio" name="Propertyfor" onchange="RentSelected()"
                value="Rent/Lease" id="PropertyforRent" checked>
              <%}else{%>
                <input class="form-check-input" type="radio" name="Propertyfor" onchange="RentSelected()"
                  value="Rent/Lease" id="PropertyforRent">
                <%}%>
                  <label class="form-check-label" for="PropertyforRent">Rent</label>
          </div>
          <br>
          <br>
          <label for="Locality">Locality</label>
          <br>
          <br>
          <select class="form-control w-50" name="Locality" id="Locality" required>
            <% if(property && property.locality=="Roadpali" ){ %>
              <option value="Roadpali" selected>Roadpali</option>
              <%}else{%>
                <option value="Roadpali">Roadpali</option>
                <%}%>
                  <% if(property && property.locality=="Kalamboli" ){ %>
                    <option value="Kalamboli" selected>Kalamboli</option>
                    <%}else{%>
                      <option value="Kalamboli">Kalamboli</option>
                      <%}%>
                        <% if(property && property.locality=="Kharghar" ){ %>
                          <option value="Kharghar" selected>Kharghar</option>
                          <%}else{%>
                            <option value="Kharghar">Kharghar</option>
                            <%}%>
          </select>
          <br>
          <h3>Property Features</h3>
          <label for="name">Name</label>
          <br>
          <% if(property && property.name){ %>
            <input type="text" placeholder="Enter name of property" class="form-control w-50" id="name" name="name"
              value="<%= property.name %>" required>
            <%}else{%>
              <input type="text" placeholder="Enter name of property" class="form-control w-50" id="name"
                name="name" required>
              <%}%>
          <br>
          <br>
          <label for="address">Address</label>
          <br>
          <% if(property && property.address){ %>
          <textarea class="form-control w-50" name="address" placeholder="Enter Address" id="address" cols="30" rows="3"
            required> <%= property.address %> </textarea>
            <%}else{%>
              <textarea class="form-control w-50" name="address" placeholder="Enter Address" id="address" cols="30" rows="3"
                required></textarea>
              <%}%>
          <br>
          <br>
          <label for="locatedInside">Located Inside</label>
          <br>
          <select class="form-control w-50" name="locatedInside" id="locatedInside" required>
            <% if(property && property.locatedInside == ""){ %>
            <option value="" selected>Select</option>
            <%}else{%>
              <option value="">Select</option>
              <%}%>
              <% if(property && property.locatedInside == "IT Park"){ %>
            <option value="IT Park" selected> IT Park </option>
            <%}else{%>
              <option value="IT Park"> IT Park </option>
              <%}%>
              <% if(property && property.locatedInside == "Business Park"){ %>
            <option value="Business Park" selected> Business Park </option>
            <%}else{%>
              <option value="Business Park"> Business Park </option>
              <%}%>
              <% if(property && property.locatedInside == "Other"){ %>
            <option value="Other" selected> Other </option>
            <%}else{%>
              <option value="Other"> Other </option>
              <%}%>
          </select>
          <br>
          <br>
          <label for="zoneType">Zone Type</label>
          <br>
          <select class="form-control w-50" name="zoneType" id="zoneType" required>
            <% if(property && property.zoneType == ""){ %>
            <option value="" selected>Select</option>
            <%}else{%>
              <option value="">Select</option>
              <%}%>
              <% if(property && property.zoneType == "Industrial"){ %>
            <option value="Industrial" selected> Industrial </option>
            <%}else{%>
              <option value="Industrial"> Industrial </option>
              <%}%>
              <% if(property && property.zoneType == "Residential"){ %>
            <option value="Residential" selected> Residential </option>
            <%}else{%>
              <option value="Residential"> Residential </option>
              <%}%>
              <% if(property && property.zoneType == "Transport and Communication"){ %>
            <option value="Transport and Communication" selected> Transport and Communication </option>
            <%}else{%>
              <option value="Transport and Communication"> Transport and Communication </option>
              <%}%>
              <% if(property && property.zoneType == "Public Utilities"){ %>
            <option value="Public Utilities" selected> Public Utilities </option>
            <%}else{%>
              <option value="Public Utilities"> Public Utilities </option>
              <%}%>
              <% if(property && property.zoneType == "Public and Semi Public Use"){ %>
            <option value="Public and Semi Public Use" selected> Public and Semi Public Use </option>
            <%}else{%>
              <option value="Public and Semi Public Use"> Public and Semi Public Use </option>
              <%}%>
              <% if(property && property.zoneType == "Open Spaces"){ %>
            <option value="Open Spaces" selected> Open Spaces </option>
            <%}else{%>
              <option value="Open Spaces"> Open Spaces </option>
              <%}%>
            <% if(property && property.zoneType == "Agricultural Zone"){ %>
            <option value="Agricultural Zone" selected> Agricultural Zone </option>
            <%}else{%>
              <option value="Agricultural Zone"> Agricultural Zone </option>
              <%}%>
            <% if(property && property.zoneType == "Special Economic Zone"){ %>
            <option value="Special Economic Zone" selected> Special Economic Zone </option>
            <%}else{%>
              <option value="Special Economic Zone"> Special Economic Zone </option>
              <%}%>
            <% if(property && property.zoneType == "Natural Conservation Zone"){ %>
            <option value="Natural Conservation Zone" selected> Natural Conservation Zone </option>
            <%}else{%>
              <option value="Natural Conservation Zone"> Natural Conservation Zone </option>
              <%}%>
            <% if(property && property.zoneType == "Government Use"){ %>
            <option value="Government Use" selected> Government Use </option>
            <%}else{%>
              <option value="Government Use"> Government Use </option>
              <%}%>
            <% if(property && property.zoneType == "Other"){ %>
            <option value="Other" selected> Other </option>
            <%}else{%>
              <option value="Other"> Other </option>
              <%}%>
          </select>
          <br>
          <br>
          <% if(property ){ %>
               <% if(property.propertyType == "Commercial Office Space" ){%>
          <div id="Office" class="">
            <label for="minSeats">Minimum Seats</label>
            <br>
            <% if(property.officeSetup.minSeats){%>
            <input type="text" value="<%= property.officeSetup.minSeats %>" placeholder="Enter minimum Seats" class="form-control w-50" id="minSeats" name="minSeats"
              required>
              <%}else{%>
                <input type="text" placeholder="Enter minimum Seats" class="form-control w-50" id="minSeats" name="minSeats"
                required>
                <%}%>
            <br>
            <br>
            <label for="maxSeats">Maximum Seats</label>
            <br>
            <% if(property.officeSetup.maxSeats){%>
            <input type="text" value="<%= property.officeSetup.maxSeats %>" placeholder="Enter maximum Seats" class="form-control w-50" id="maxSeats" name="maxSeats"
              required>
              <%}else{%>
                <input type="text" placeholder="Enter maximum Seats" class="form-control w-50" id="maxSeats" name="maxSeats"
              required>
                <%}%>
            <br>
            <br>
            <label for="noOfCabins">Number Of Cabins</label>
            <br>
            <% if(property.officeSetup.noOfCabins){%>
            <input type="text" placeholder="Enter number Of Cabins" class="form-control w-50" id="noOfCabins"
              name="noOfCabins" value="<%= property.officeSetup.noOfCabins %>" required>
              <%}else{%>
                <input type="text" placeholder="Enter number Of Cabins" class="form-control w-50" id="noOfCabins"
                  name="noOfCabins" required>
                <%}%>
            <br>
            <br>
            <label for="noOfMeetingRooms">Number Of Meeting Rooms</label>
            <br>
            <% if(property.officeSetup.noOfMeetingRooms){%>
            <input type="text" onchange="meetingRooms()" placeholder="Enter number Of Meeting Rooms"
              class="form-control w-50" value="<%= property.officeSetup.noOfMeetingRooms %>" id="noOfMeetingRooms" name="noOfMeetingRooms" required>
              <%}else{%>
                <input type="text" onchange="meetingRooms()" placeholder="Enter number Of Meeting Rooms"
              class="form-control w-50" id="noOfMeetingRooms" name="noOfMeetingRooms" required>
                <%}%>
            <br>
            <br>
            <div class="form-check form-check-inline">
              <% if(property.conferenceRoom == true){%>
              <input class="form-check-input" type="checkbox" onchange="conferenceRooms(this)" name="conferenceRoom"
                value="true" id="conferenceRoom"  checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" onchange="conferenceRooms(this)" name="conferenceRoom"
                value="true" id="conferenceRoom" >
                  <%}%>
              <label class="form-check-label font-weight-bold" style="text-decoration: underline;"
                for="conferenceRoom">Conference Room</label>
            </div>
            <br>
            <br>
            <div class="form-check form-check-inline">
              <% if(property.receptionArea == true){%>
              <input class="form-check-input" type="checkbox" onchange="receptionAreas(this)" name="receptionArea"
                value="true" id="receptionArea"  checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" onchange="receptionAreas(this)" name="receptionArea"
                    value="true" id="receptionArea" >
                  <%}%>
              <label class="form-check-label font-weight-bold" style="text-decoration: underline;"
                for="receptionArea">Reception Area</label>
            </div>
            <br>
            <br>
            <label for="pantryTypes">Pantry Types</label>
            <br>
            <select class="form-control w-50" name="pantryTypes" id="pantryTypes" required>
              <% if(property.pantryType.pantryTypes == ""){%>
              <option value="" selected>Select</option>
              <%}else{%>
                <option value="">Select</option>
                <%}%>
              <% if(property.pantryType.pantryTypes == "Private"){%>
              <option value="Private" selected> Private </option>
              <%}else{%>
                <option value="Private"> Private </option>
                <%}%>
                <% if(property.pantryType.pantryTypes == "Shared"){%>
              <option value="Shared" selected> Shared </option>
              <%}else{%>
                <option value="Shared"> Shared </option>
                <%}%>
                <% if(property.pantryType.pantryTypes == "Not Avaliable"){%>
              <option value="Not Avaliable" selected> Not Avaliable </option>
              <%}else{%>
                <option value="Not Avaliable"> Not Avaliable </option>
                <%}%>
            </select>
            <br>
            <br>
            <label for="pantrySize">Pantry Size</label>
            <br>
            <% if(property.pantryType.pantrySize){%>
            <input type="text" placeholder="Enter size" class="form-control w-50" id="pantrySize" value="<%= property.pantryType.pantrySize %>" name="pantrySize"
              required >
              <%}else{%>
                <input type="text" placeholder="Enter size" class="form-control w-50" id="pantrySize" name="pantrySize"
                  required>
                <%}%>
          </div>

          <div id="showroom" class="d-none">
            <label for="balconies">Balconies</label>
            <br>
            <input type="text" placeholder="Enter number of balconies" onchange="balcony()" class="form-control w-50"
              id="balconies" name="balconies" >
          </div>

          <div id="Shop" class="d-none">
            <label for="shopbalconies">Balconies</label>
            <br>
            <input type="text" placeholder="Enter number of balconies" onchange="shopbalcony()"
              class="form-control w-50" id="shopbalconies" name="shopbalconies" >
          </div>

          <div id="extras" class="">
            <h3>Washrooms</h3>
            <div class="form-check form-check-inline">
              <% if(property.washrooms.isAvaliable == true){ %>
              <input class="form-check-input" type="checkbox" onchange="washroomAvailible()" name="WashroomisAvaliable"
                value="true" id="WashroomisAvaliable" checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" onchange="washroomAvailible()" name="WashroomisAvaliable"
                    value="true" id="WashroomisAvaliable" >
                  <%}%>
              <label class="form-check-label " for="WashroomisAvaliable">Washrooms Available</label>
            </div>
            <br>
            <br>
            <% if(property.washrooms.isAvaliable == true){ %>
            <div id="washroomQuantity" class="">
              <label for="quantity">Quantity</label>
              <br>
              <% if(property.washrooms.quantity){ %>
              <input type="text" onchange="washroomQuantity()" placeholder="Enter number of quantity"
                class="form-control w-50" id="quantity" name="quantity" value="<%= property.washrooms.quantity %>">
                <%}else{%>
                  <input type="text" onchange="washroomQuantity()" placeholder="Enter number of quantity"
                    class="form-control w-50" id="quantity" name="quantity" >
                  <%}%>
              <br>
            </div>
            <%}else{%>
              <div id="washroomQuantity" class="d-none">
                <label for="quantity">Quantity</label>
                <br>
                <input type="text" onchange="washroomQuantity()" placeholder="Enter number of quantity"
                  class="form-control w-50" id="quantity" name="quantity">
                <br>
              </div>
            <%}%>
            <h3>facilities</h3>
            <div class="form-check form-check-inline">
              <% if(property.facilities.includes("Furnished")){ %>
              <input class="form-check-input" type="checkbox" name="facilities" value="Furnished" id="Furnished" checked>
              <%}else{%>
                <input class="form-check-input" type="checkbox" name="facilities" value="Furnished" id="Furnished">
                <%}%>
              <label class="form-check-label" for="Furnished">Furnished</label>
            </div>
            <div class="form-check form-check-inline">
              <% if(property.facilities.includes("Central Air Condition")){ %>
              <input class="form-check-input" type="checkbox" name="facilities" value="Central Air Condition"
                id="centralAirCondition" checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" name="facilities" value="Central Air Condition"
                    id="centralAirCondition">
                  <%}%>
              <label class="form-check-label" for="centralAirCondition">Central Air Condition</label>
            </div>
            <div class="form-check form-check-inline">
              <% if(property.facilities.includes("Oxygen Duct")){ %>
              <input class="form-check-input" type="checkbox" name="facilities" value="Oxygen Duct" id="OxygenDuct" checked>
              <%}else{%>
                <input class="form-check-input" type="checkbox" name="facilities" value="Oxygen Duct" id="OxygenDuct">
                <%}%>
              <label class="form-check-label" for="OxygenDuct">Oxygen Duct</label>
            </div>
            <div class="form-check form-check-inline">
              <% if(property.facilities.includes("UPS")){ %>
              <input class="form-check-input" type="checkbox" name="facilities" value="UPS" id="UPS" checked>
              <%}else{%>
                <input class="form-check-input" type="checkbox" name="facilities" value="UPS" id="UPS">
                <%}%>
              <label class="form-check-label" for="UPS">UPS</label>
            </div>
            <h3>Fire Safety Measures</h3>
            <div class="form-check form-check-inline">
              <% if(property.fireSafetyMeasures.includes("Fire Extingusher")){ %>
              <input class="form-check-input" type="checkbox" name="fireSafetyMeasures" value="Fire Extingusher"
                id="fireExtingusher" checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" name="fireSafetyMeasures" value="Fire Extingusher"
                    id="fireExtingusher">
                  <%}%>
              <label class="form-check-label" for="fireExtingusher">Fire Extingusher</label>
            </div>
            <div class="form-check form-check-inline">
              <% if(property.fireSafetyMeasures.includes("Fire Sensors")){ %>
              <input class="form-check-input" type="checkbox" name="fireSafetyMeasures" value="Fire Sensors"
                id="fireSensors" checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" name="fireSafetyMeasures" value="Fire Sensors"
                    id="fireSensors">
                  <%}%>
              <label class="form-check-label" for="fireSensors">Fire Sensors</label>
            </div>
            <div class="form-check form-check-inline">
              <% if(property.fireSafetyMeasures.includes("Sprinklers")){ %>
              <input class="form-check-input" type="checkbox" name="fireSafetyMeasures" value="Sprinklers"
                id="Sprinklers" checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" name="fireSafetyMeasures" value="Sprinklers"
                    id="Sprinklers">
                  <%}%>
              <label class="form-check-label" for="Sprinklers">Sprinklers</label>
            </div>
            <div class="form-check form-check-inline">
              <% if(property.fireSafetyMeasures.includes("Firehouse")){ %>
              <input class="form-check-input" type="checkbox" name="fireSafetyMeasures" value="Firehouse"
                id="Firehouse" checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" name="fireSafetyMeasures" value="Firehouse"
                    id="Firehouse">
                  <%}%>
              <label class="form-check-label" for="Firehouse">Firehouse</label>
            </div>

            <h3>Floor Details</h3>
            <label for="totalFloors">Total Floors</label>
            <br>  
            <% if(property.floorDetails.totalFloors){ %>
            <input type="text" onchange="totalFloor()" placeholder="Enter total number of Floors"
              class="form-control w-50" value="<%= property.floorDetails.totalFloors %>" id="totalFloors" name="totalFloors" required>
              <%}else{%>
                <input type="text" onchange="totalFloor()" placeholder="Enter total number of Floors"
              class="form-control w-50" id="totalFloors" name="totalFloors" required>
                <%}%>
            <br>
            <br>
            <label for="yourFloor">Your Floor</label>
            <br>
            <% if(property.floorDetails.yourFloor){ %>
            <input type="text" onchange="FloorNo()" placeholder="Enter Your Floors" class="form-control w-50"
              id="yourFloor" value="<%= property.floorDetails.yourFloor %>" name="yourFloor" required>
              <%}else{%>
                <input type="text" onchange="FloorNo()" placeholder="Enter Your Floors" class="form-control w-50"
              id="yourFloor" name="yourFloor" required>
                <%}%>
            <br>
            <br>
            <label for="noOfStaircases">No Of Staircases</label>
            <br>
            <% if(property.floorDetails.noOfStaircases){ %>
            <input type="text" placeholder="Enter total number of Staircases" class="form-control w-50"
              id="noOfStaircases" value="<%= property.floorDetails.noOfStaircases  %>" name="noOfStaircases" required>
              <%}else{%>
                <input type="text" placeholder="Enter total number of Staircases" class="form-control w-50"
                  id="noOfStaircases" name="noOfStaircases" required>
                <%}%>
            <h3>Lifts</h3>
            <div class="form-check form-check-inline">
              <% if(property.lifts.isAvaliable == true){ %>
              <input class="form-check-input" type="checkbox" onchange="liftAvailible()" name="liftisAvaliable"
                value="true" id="liftisAvaliable" checked>
                <%}else{%>
                  <input class="form-check-input" type="checkbox" onchange="liftAvailible()" name="liftisAvaliable"
                value="true" id="liftisAvaliable" >
                  <%}%>
              <label class="form-check-label " for="liftisAvaliable">Lifts</label>
            </div>
            <br>
            <br>
            <% if(property.lifts.isAvaliable == true){ %>
            <div id="liftQuantity" class="">
              <label for="passengerLifts">Passenger Lifts</label>
              <br>
              <% if(property.lifts.passengerLifts){ %>
              <input type="text" placeholder="Enter number of Passenger Lifts" class="form-control w-50"
